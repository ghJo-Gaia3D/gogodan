        function setNumber(node) {
            for(let s = 0; s < 5; s++) {
                let value = s + 1;
                let newOption = document.createElement('option');
                newOption.setAttribute('value', value);
                newOption.text = value;
                node.appendChild(newOption);
            }
        }

